    "def mystery_sum(x):\n",
    "    try:\n",
    "        return sum([(((-1)**n)*n)/(4*n)*(x+3)**n for n in range(1,1000000)])\n",
    "    except OverflowError:\n",
    "        return \"Infinity\"\n",
    "\n",
    "for x in range(-10,11):\n",
    "    print(x,mystery_sum(x))\n"
